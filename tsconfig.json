{
  "extends": "astro/tsconfigs/base",

  "compilerOptions": {

    "checkJs": false,

    "strict": false,

    "jsx": "react-jsx",
    "jsxImportSource": "react",

    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@layouts/*": ["src/layouts/*"],
      "@Projects/*": ["src/data/Projects/*"],
      "@Data/*": ["src/data/*"]
    }
  }
}